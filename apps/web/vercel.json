{
  "buildCommand": "cd ../.. && pnpm run build --filter=@rebridge/web",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install",
  "regions": ["icn1"],
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://rebridge.vercel.app"
  }
}